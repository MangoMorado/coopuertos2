<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
    configVersion="3"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="https://www.phpdoc.org"
    xsi:noNamespaceSchemaLocation="https://www.phpdoc.org/schema/3.1/phpdoc.xsd"
>
    <paths>
        <output>docs/api</output>
        <cache>docs/api/.phpdoc/cache</cache>
    </paths>
    <version number="1.0.0">
        <api>
            <source dsn=".">
                <path>app</path>
            </source>
            <default-package-name>Coopuertos</default-package-name>
            <include-source>true</include-source>
        </api>
    </version>
    <template name="default"/>
</phpdocumentor>
